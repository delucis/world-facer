{
  "name": "world-facer",
  "description": "Scrape and present various data, preparing for Rainy Days",
  "author": "Chris Swithinbank <swithinbank@gmail.com> (http://chrisswithinbank.net/)",
  "license": "GPL-3.0",
  "scripts": {
    "bootstrap": "node ./lib/bootstrap",
    "sketch": "node --max-old-space-size=8192 ./lib/face-sketcher",
    "make-cues": "node ./lib/cue-speaker",
    "read-news": "node ./lib/news-reader",
    "prerelease": "npm t",
    "release": "standard-version -s",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "standard",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@delucis/aproba": "^3.0.0",
    "@perl/qx": "^1.0.3",
    "any-shell-escape": "^0.1.1",
    "are-we-there-yet": "^1.1.5",
    "big-json": "^2.0.2",
    "canvas-sketch-util": "^1.8.0",
    "cheerio": "^1.0.0-rc.3",
    "count-words": "^1.0.12",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "eases": "^1.0.8",
    "franc-min": "^4.1.0",
    "gauge": "^2.7.4",
    "image-type": "^4.1.0",
    "import-fresh": "^3.0.0",
    "inquirer": "^6.3.1",
    "instapaper": "github:delucis/node-instapaper",
    "is-absolute-url": "^3.0.0",
    "make-dir": "^3.0.0",
    "nlcst-to-string": "^2.0.2",
    "ora": "^3.4.0",
    "p-queue": "^5.0.0",
    "pellicola": "^0.9.1",
    "pretty-ms": "^5.0.0",
    "profanities": "^2.10.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "resemblejs": "^3.1.0",
    "retext": "^6.0.1",
    "retext-keywords": "^4.0.2",
    "sbd": "^1.0.14",
    "slugg": "^1.2.1",
    "stopwords-iso": "^0.4.0",
    "strip": "^3.0.0"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "commitizen": "3.1.1",
    "coveralls": "3.0.3",
    "cz-conventional-changelog": "2.1.0",
    "ninos": "^3.0.0",
    "nyc": "14.1.0",
    "standard": "12.0.1",
    "standard-version": "6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delucis/world-facer.git"
  },
  "bugs": "https://github.com/delucis/world-facer/issues",
  "homepage": "https://npmjs.com/package/world-facer"
}
