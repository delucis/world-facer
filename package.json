{
  "name": "world-facer",
  "description": "Scrape and present various data, preparing for Rainy Days",
  "author": "Chris Swithinbank <swithinbank@gmail.com> (http://chrisswithinbank.net/)",
  "license": "GPL-3.0",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "node --max-old-space-size=8192 ./lib/bootstrap",
    "sketch": "node --max-old-space-size=8192 ./lib/face-sketcher",
    "make-cues": "node ./lib/cue-speaker",
    "read-news": "node ./lib/news-reader",
    "serve-news": "node ./lib/news-server",
    "prerelease": "npm t",
    "release": "standard-version -s",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "standard",
    "test": "nyc --reporter=lcov --reporter=text ava"
  },
  "dependencies": {
    "@delucis/aproba": "^3.0.0",
    "@perl/qx": "^1.1.0",
    "any-shell-escape": "^0.1.1",
    "are-we-there-yet": "^2.0.0",
    "big-json": "^3.1.0",
    "canvas-sketch-util": "^1.10.0",
    "cheerio": "^1.0.0-rc.10",
    "count-words": "^1.0.12",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "eases": "^1.0.8",
    "express": "^4.17.1",
    "franc-min": "^5.0.0",
    "gauge": "^3.0.1",
    "image-type": "^4.1.0",
    "import-fresh": "^3.3.0",
    "inquirer": "^8.1.2",
    "instapaper": "github:delucis/node-instapaper",
    "is-absolute-url": "^3.0.3",
    "make-dir": "^3.1.0",
    "ngrok": "^4.2.2",
    "nlcst-to-string": "^2.0.4",
    "ora": "^5.4.1",
    "p-queue": "^5.0.0",
    "pellicola": "^0.12.0",
    "pretty-ms": "^7.0.1",
    "profanities": "^2.14.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "resemblejs": "^3.2.5",
    "retext": "^7.0.1",
    "retext-keywords": "^6.0.0",
    "retext-pos": "^3.0.0",
    "sbd": "^1.0.19",
    "slugg": "^1.2.1",
    "stackblur-canvas": "^2.5.0",
    "stopwords-iso": "^1.1.0",
    "strip": "^3.0.0",
    "user-agents": "^1.0.774"
  },
  "devDependencies": {
    "ava": "2.4.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "ninos": "^3.0.0",
    "nyc": "15.1.0",
    "standard": "16.0.3",
    "standard-version": "9.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delucis/world-facer.git"
  },
  "bugs": "https://github.com/delucis/world-facer/issues",
  "homepage": "https://npmjs.com/package/world-facer"
}
